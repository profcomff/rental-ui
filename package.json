{
	"name": "my-app-frontend",
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vue-tsc && vite build",
		"docker-build": "vue-tsc && vite build --base=/ui",
		"preview": "vite preview",
		"lint": "eslint src/",
		"lint:deadcode": "knip --exclude binaries,dependencies,unlisted",
		"lint:circular": "dpdm --exit-code circular:1 --no-tree --no-warning --progress false --transform ./src/main.ts",
		"format": "prettier src/ --write",
		"stylelint": "stylelint 'src/**/*.{vue,css}' --fix",
		"check": "vue-tsc && pnpm lint && pnpm format && pnpm stylelint",
		"check:hard": "vue-tsc && pnpm lint && pnpm lint:deadcode && pnpm lint:circular && pnpm format && pnpm stylelint",
		"test": "echo ok"
	},
	"dependencies": {
		"@fontsource/roboto": "^5.2.9",
		"pinia": "^3.0.4",
		"query-string": "^9.3.1",
		"vue": "^3.5.26",
		"vue-router": "^4.6.4"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.3.3",
		"@eslint/js": "^9.39.2",
		"@mapbox/stylelint-processor-arbitrary-tags": "^0.4.0",
		"@mdi/font": "^7.4.47",
		"@profcomff/api-uilib": "2025.12.21-test",
		"@types/node": "^25.0.3",
		"@typescript-eslint/eslint-plugin": "^8.50.1",
		"@typescript-eslint/parser": "^8.50.1",
		"@vitejs/plugin-vue": "^6.0.3",
		"@vue/eslint-config-typescript": "^14.6.0",
		"dpdm": "^3.14.0",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-prettier": "^5.5.4",
		"eslint-plugin-vue": "^10.6.2",
		"knip": "^5.76.3",
		"openapi-typescript": "^7.10.1",
		"postcss-html": "^1.8.0",
		"postcss-preset-env": "^10.5.0",
		"prettier": "^3.7.4",
		"sass-embedded": "^1.97.1",
		"stylelint": "^16.26.1",
		"stylelint-config-recommended": "^17.0.0",
		"stylelint-config-recommended-vue": "^1.6.1",
		"stylelint-config-standard": "^39.0.1",
		"stylelint-use-nesting": "^6.0.1",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.50.1",
		"unplugin-fonts": "^1.4.0",
		"vite": "^7.3.0",
		"vite-plugin-stylelint": "^6.0.2",
		"vite-plugin-vuetify": "^2.1.2",
		"vite-svg-loader": "^5.1.0",
		"vue-tsc": "^3.2.1",
		"vuetify": "^3.11.4"
	}
}