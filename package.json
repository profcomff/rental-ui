{
	"name": "my-app-frontend",
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vue-tsc && vite build",
		"docker-build": "vue-tsc && vite build --base=/ui",
		"preview": "vite preview",
		"lint": "eslint src/",
		"lint:deadcode": "knip --exclude binaries,dependencies,unlisted",
		"lint:circular": "dpdm --exit-code circular:1 --no-tree --no-warning --progress false --transform ./src/main.ts",
		"format": "prettier src/ --write",
		"stylelint": "stylelint 'src/**/*.{vue,css}' --fix",
		"check": "vue-tsc && pnpm run format && pnpm run lint && pnpm run stylelint",
		"test": "echo ok"
	},
	"dependencies": {
		"pinia": "^3.0.2",
		"query-string": "^9.1.1",
		"vue": "^3.5.13",
		"vue-router": "^4.5.1"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.25.1",
		"@mapbox/stylelint-processor-arbitrary-tags": "^0.4.0",
		"@mdi/font": "^7.4.47",
		"@profcomff/api-uilib": "^2025.9.18",
		"@types/node": "^22.15.3",
		"@typescript-eslint/eslint-plugin": "^8.31.1",
		"@typescript-eslint/parser": "^8.31.1",
		"@vitejs/plugin-vue": "^5.2.3",
		"@vue/eslint-config-typescript": "^14.5.0",
		"dpdm": "^3.14.0",
		"eslint": "^9.25.1",
		"eslint-config-prettier": "^10.1.2",
		"eslint-plugin-prettier": "^5.2.6",
		"eslint-plugin-vue": "^9.33.0",
		"knip": "^5.51.1",
		"openapi-typescript": "^7.6.1",
		"postcss-html": "^1.8.0",
		"postcss-preset-env": "^10.1.6",
		"prettier": "^3.5.3",
		"stylelint": "^16.19.1",
		"stylelint-config-recommended": "^15.0.0",
		"stylelint-config-recommended-vue": "^1.6.0",
		"stylelint-config-standard": "^37.0.0",
		"stylelint-use-nesting": "^6.0.0",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.31.1",
		"vite": "^6.3.4",
		"vite-plugin-stylelint": "^6.0.0",
		"vite-plugin-vuetify": "^2.1.1",
		"vite-svg-loader": "^5.1.0",
		"vue-tsc": "^2.2.10",
		"vuetify": "^3.8.3"
	}
}
